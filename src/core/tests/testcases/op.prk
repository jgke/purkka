struct Tuple {left: i32, right: i32};

operator 11 +=+ (left: Tuple, right: Tuple) => Tuple {
    left: left.left + right.left,
    right: left.right + right.right
};

fun main() {
    let left = Tuple { left: 0, right: 0 };
    let right = Tuple { left: 1, right: 1 };
    let mid = left +=+ right;
    return mid.left;
}
